{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "image": "automaticacore/automatica.slave",
  "imageTag": "${IMAGE_TAG}",
  "dockerRegistry": "https://registry.hub.docker.com",
  "pollIntervalMinutes": 5,
  "mounts": [
    "/var/lib/slave/:/app/slave/config/",
    "/var/log/slave/:/app/slave/logs/"
  ]
}
